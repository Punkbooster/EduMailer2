<p id="notice"><%= notice %></p>

<h1>Listing Requests</h1>

<table>
  <thead>
    <tr>
      <th>Imie</th>
      <th>Nazwisko</th>
      <th>Email</th>
      <th>Kierunek</th>
      <th>Rok studji</th>
      <th>Nr legitymacji</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.imie %></td>
        <td><%= request.nazwisko %></td>
        <td><%= request.email %></td>
        <td><%= request.kierunek %></td>
        <td><%= request.rok_studji %></td>
        <td><%= request.nr_legitymacji %></td>
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request', new_request_path %>
